; Initialisation du programme
     TSTO #11
     BOV pile_pleine
     ADDSP #6

; ####################################
; # Creation des tables des methodes #
; ####################################

; Table des méthodes de la classe Object
     LOAD #null, R0
     STORE R0, 1 (GB)
     LOAD code.Object.equals, R0
     STORE R0, 2 (GB)

; Table des méthode de la classe Rec
     LEA 1 (GB), R0
     STORE R0, 3 (GB)
     LOAD code.Object.equals, R0
     STORE R0, 4 (GB)
     LOAD code.Rec.ackermann, R0
     STORE R0, 5 (GB)

; #######################
; # Programme principal #
; #######################

; Declarations ligne n° 29.
     NEW #1, R2
     BOV tas_plein
     LEA 3 (GB), R0
     STORE R0, 0 (R2)
     PUSH R2
     BSR init.Rec
     POP R2
     STORE R2, 6 (LB)


; Instruction ligne n° 31.
     WSTR "Ackermann(5,20) = "
     ADDSP #3
     LOAD 6 (LB), R1
     CMP #null, R1
     BEQ dereferencement_null
     STORE R1, 0 (SP)
     LOAD #5, R1
     STORE R1, -1 (SP)
     LOAD #20, R1
     STORE R1, -2 (SP)
     LOAD 0 (SP), R0
     LOAD 0 (R0), R0
     BSR 2 (R0)
     SUBSP #3
     LOAD R0, R2
     LOAD R2, R1
     WINT
     WNL

; Fin du programme principal
     HALT


; #######################
; # Gestion des erreurs #
; #######################

debordement :
     WSTR "Erreur : dépassement de capacité"
     WNL
     ERROR

tas_plein :
     WSTR "Erreur : tas plein"
     WNL
     ERROR

pile_pleine :
     WSTR "Erreur : pile pleine"
     WNL
     ERROR

dereferencement_null :
     WSTR "Erreur : dereferencement de null"
     WNL
     ERROR

conversion_impossible :
     WSTR "Erreur : conversion impossible"
     WNL
     ERROR

; ############################
; # Code de la classe Object #
; ############################

; Méthode Object.equals
code.Object.equals :
     LOAD -2 (LB), R0
     CMP -3 (LB), R0
     SEQ R0
     RTS

; #########################
; # Code de la classe Rec #
; #########################

; Méthode Rec.ackermann
code.Rec.ackermann :
     TSTO #9
     BOV pile_pleine
     ADDSP #1
     STORE R2, 0 (SP)

; Instruction ligne n° 15.
if.0.0 :
     LOAD -3 (LB), R2

; Comparaison EQ
     CMP #0, R2
     SEQ R2
     CMP #0, R2
     BEQ else.0

; Instruction ligne n° 16.
     LOAD -4 (LB), R2
     ADD #1, R2
     LOAD R2, R0
     BRA fin.Rec.ackermann
     BRA fin.0
else.0 :

; Instruction ligne n° 19.
if.1.0 :
     LOAD -4 (LB), R2

; Comparaison EQ
     CMP #0, R2
     SEQ R2
     CMP #0, R2
     BEQ else.1

; Instruction ligne n° 20.
     ADDSP #3
     LOAD -2 (LB), R1
     STORE R1, 0 (SP)
     LOAD -3 (LB), R2
     SUB #1, R2
     STORE R2, -1 (SP)
     LOAD #1, R1
     STORE R1, -2 (SP)
     LOAD 0 (SP), R0
     LOAD 0 (R0), R0
     BSR 2 (R0)
     SUBSP #3
     LOAD R0, R2
     LOAD R2, R0
     BRA fin.Rec.ackermann
     BRA fin.1
else.1 :

; Instruction ligne n° 23.
     ADDSP #3
     LOAD -2 (LB), R1
     STORE R1, 0 (SP)
     LOAD -3 (LB), R2
     SUB #1, R2
     STORE R2, -1 (SP)
     ADDSP #3
     LOAD -2 (LB), R1
     STORE R1, 0 (SP)
     LOAD -3 (LB), R1
     STORE R1, -1 (SP)
     LOAD -4 (LB), R2
     SUB #1, R2
     STORE R2, -2 (SP)
     LOAD 0 (SP), R0
     LOAD 0 (R0), R0
     BSR 2 (R0)
     SUBSP #3
     LOAD R0, R2
     STORE R2, -2 (SP)
     LOAD 0 (SP), R0
     LOAD 0 (R0), R0
     BSR 2 (R0)
     SUBSP #3
     LOAD R0, R2
     LOAD R2, R0
     BRA fin.Rec.ackermann
fin.1 :
fin.0 :
     WSTR "Erreur : sortie de la méthode Rec.ackermann sans return"
     WNL
     ERROR
fin.Rec.ackermann :
     LOAD 0 (SP), R2
     SUBSP #1
     RTS

; Initialisation des champs de la classe Rec
init.Rec :
     RTS
